{
	"version": 2,
	"title": "Maroon",
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [
				{
					"colors": [ "#000000", "#ffffff" ],
					"slug": "foreground-and-background",
					"name": "Foreground and background"
				},
				{
					"colors": [ "#000000", "#ffe2c7" ],
					"slug": "foreground-and-secondary",
					"name": "Foreground and secondary"
				},
				{
					"colors": [ "#000000", "#f6f6f6" ],
					"slug": "foreground-and-tertiary",
					"name": "Foreground and tertiary"
				},
				{
					"colors": [ "#1a4548", "#ffffff" ],
					"slug": "primary-and-background",
					"name": "Primary and background"
				},
				{
					"colors": [ "#1a4548", "#ffe2c7" ],
					"slug": "primary-and-secondary",
					"name": "Primary and secondary"
				},
				{
					"colors": [ "#1a4548", "#f6f6f6" ],
					"slug": "primary-and-tertiary",
					"name": "Primary and tertiary"
				}
			],

			"palette": [
					{
						"slug": "foreground",
						"color": "#ffffff",
						"name": "Foreground"
					},
					{
						"slug": "background",
						"color": "#fff8e6",
						"name": "Background"
					},
					{
						"slug": "background-header",
						"color": "#5b0013",
						"name": "Background header"
					},
					{
						"slug": "primary",
						"color": "#000000",
						"name": "Primary"
					},
					{
						"slug": "secondary",
						"color": "#1b484b",
						"name": "Secondary"
					},
					{
						"slug": "tertiary",
						"color": "#fcb900",
						"name": "Tertiary"
					},
					{
						"slug": "quaternary",
						"color": "#938f8f",
						"name": "Quaternary"
					}
				]

		},
		"custom": {
			"spacing": {
				"small": "max(1.25rem, 2vw)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "var(--wp--custom--spacing--small, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"huge": "clamp(2.25rem, 4vw, 2.75rem)",
					"gigantic": "clamp(2.75rem, 5vw, 3.25rem)",
					"colossal": "clamp(3.25rem, 8vw, 6.25rem)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.6
				}
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "'Work Sans', -apple-system, BlinkMacSystemFont, 'Helvetica Neue', 'Helvetica', sans-serif",
					"slug": "work-sans",
					"name": "Work Sans",
					"fontFace": [
						{
							"fontFamily": "Work Sans",
							"fontDisplay": "block",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/work-sans/WorkSans-VariableFont_wght.ttf" ]
						},
						{
							"fontFamily": "Work Sans",
							"fontDisplay": "block",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/work-sans/WorkSans-VariableFont_wght.ttf" ]
						},
						{
							"fontFamily": "Work Sans",
							"fontDisplay": "block",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/work-sans/WorkSans-Italic-VariableFont_wght.ttf" ]
						},
						{
							"fontFamily": "Work Sans",
							"fontDisplay": "block",
							"fontWeight": "700",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/work-sans/WorkSans-Italic-VariableFont_wght.ttf" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"size": ".9rem",
					"fluid": false,
					 "slug": "small",
					"name": "Small"
				},
				{
					"size": "1rem",
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					},
					"slug": "normal",
					"name": "Normal"
				},
				{
					"size": "1.25rem",
					"fluid": {
						"min": "1.25rem",
						"max": "1.75rem"
					},
					"slug": "medium",
					"name": "Medium"
				},
				{
					"size": "1.5rem",
					"fluid": {
						"min": "1.5rem",
						"max": "2.25rem"
					},
					"slug": "large",
					"name": "large"
				},
				{
					"size": "1.75",
					"fluid": {
						"min": "1.75rem",
						"max": "3.25rem"
					},
					"slug": "x-large",
					"name": "x-large"
				}
			]
		},
		"layout": {
			"contentSize": "700px",
			"wideSize": "1000px"
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				},
				"core/post-author": {
					"typography": {
						"fontSize": "var(--wp--preset--font-size--small)"
					},
					"color": {
						"text": "var(--wp--preset--color--quarternary)"
					},
					"elements": {
						"link": {
							"color": {
								"text": "var(--wp--preset--color--tertiary)"
							}
						}
					}
				},
				"core/post-date": {
					"typography": {
						"fontSize": "var(--wp--preset--font-size--small)"
					},
					"color": {
						"text": "var(--wp--preset--color--quarternary)"
					},
					"elements": {
						"link": {
							"color": {
								"text": "var(--wp--preset--color--tertiary)"
							}
						}
					}
				},
				"core/post-terms": {
					"typography": {
						"fontSize": "var(--wp--preset--font-size--small)"
					},
					"color": {
						"text": "var(--wp--preset--color--quarternary)"
					},
					"elements": {
						"link": {
							"color": {
								"text": "var(--wp--preset--color--tertiary)"
							}
						}
					}
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "500"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--primary)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--colossal)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--huge)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "600",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--work-sans)",
					"fontWeight": "400",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--work-sans)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--normal)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "header-small-dark",
			"title": "Header (Dark, small)",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
